{
  "name": "pwa",
  "version": "0.0.1",
  "description": "Progresive Web App",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --port=3000 --inline --hot",
    "start:server": "NODE_ENV=development node_modules/.bin/nodemon server",
    "storybook": "node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "test": "NODE_ENV=test node_modules/.bin/jest",
    "lint:css": "node_modules/.bin/stylelint './src/**/*.styled.js'",
    "lint:js": "node_modules/.bin/eslint \"@(src|server|test)/**/*.@(js|jsx)\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transform": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/style-mock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src",
      "server"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "globals": {
      "__DEV__": true,
      "__WEB_ROOT__": "/"
    },
    "roots": [
      "<rootDir>"
    ],
    "projects": [
      "<rootDir>"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanluke/pwa.git"
  },
  "keywords": [
    "React",
    "Redux",
    "PWA"
  ],
  "author": "Lukasz Gonciarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanluke/pwa/issues"
  },
  "homepage": "https://github.com/vanluke/pwa#readme",
  "dependencies": {
    "@storybook/react": "^3.2.15",
    "body-parser": "^1.18.2",
    "camelcase-keys": "^4.1.0",
    "convict": "^4.0.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "moment": "^2.19.1",
    "offline-plugin": "^4.8.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.16.0",
    "redux0-helpers": "^1.0.0",
    "rxjs": "^5.4.3",
    "seamless-immutable": "^7.1.2",
    "styled-components": "^2.1.2",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.1.0",
    "jest-cli": "^21.1.0",
    "jest-enzyme": "^4.0.1",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.7",
    "nock": "^9.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "pwa-manifest-webpack-plugin": "^1.0.5",
    "raf": "^3.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "simple-webpack-clean-plugin": "^0.1.0",
    "sinon": "^3.3.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0",
    "sw-precache-webpack-plugin-loader": "^1.0.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "webpack-pwa-manifest": "^3.3.2"
  }
}
